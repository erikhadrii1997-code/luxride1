{
  "appId": "com.luxride.driver",
  "appName": "LuxRide Driver",
  "webDir": "www",
  "server": {
    "androidScheme": "https",
    "iosScheme": "https",
    "hostname": "app.luxride.com"
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#1a1a1a",
      "showSpinner": false,
      "androidSpinnerStyle": "small",
      "iosSpinnerStyle": "small",
      "spinnerColor": "#ffffff"
    },
    "StatusBar": {
      "style": "dark",
      "backgroundColor": "#1a1a1a"
    }
  },
  "android": {
    "allowMixedContent": true,
    "backgroundColor": "#ffffff"
  },
  "ios": {
    "contentInset": "automatic",
    "backgroundColor": "#ffffff"
  }
}
